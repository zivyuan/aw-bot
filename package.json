{
  "name": "alien-worlds-robot",
  "version": "0.1.0",
  "description": "A miner robot for Alien Worlds.",
  "engines": {
    "node": ">= 16.13 <17"
  },
  "devDependencies": {
    "@types/dingtalk-robot-sender": "^1.1.1",
